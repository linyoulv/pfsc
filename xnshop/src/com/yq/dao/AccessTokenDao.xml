<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yq.dao.AccessTokenDao">
    
    
    <update id="update" parameterType="java.util.Map">
    update access_token set access_token=#{access_token} , add_time=#{add_time} where id=1
    </update>
    
    <select id="listById" resultType="com.yq.entity.AccessToken">
    select * from access_token where id=1
    </select>
    
</mapper>